{
  "name": "fuzzysort",
  "version": "3.1.0",
  "author": "farzher",
  "license": "MIT",
  "description": "Fast SublimeText-like fuzzy search for JavaScript",
  "keywords": [
    "fuzzy search",
    "fuzzy",
    "search",
    "filter",
    "javascript",
    "node",
    "fast",
    "sublime"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farzher/fuzzysort.git"
  },
  "type": "module",
  "main": "./dist/fuzzysort.cjs",
  "module": "./dist/fuzzysort.js",
  "exports": {
    ".": {
      "import": "./dist/fuzzysort.js",
      "require": "./dist/fuzzysort.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "node test/test.js",
    "test-min": "node test/test.js min",
    "build": "tsdown",
    "postbuild": "cp src/fuzzysort.d.ts dist/fuzzysort.d.ts && cp src/fuzzysort.d.ts dist/fuzzysort.d.cts",
    "dev": "npm run build -- --watch",
    "pretest": "npm run build"
  },
  "files": [
    "dist/*",
    "src/fuzzysort.js"
  ],
  "devDependencies": {
    "tsdown": "^0.14.2"
  }
}
