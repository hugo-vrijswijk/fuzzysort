{
  "name": "fuzziersort",
  "version": "4.0.1",
  "author": "farzher",
  "license": "MIT",
  "description": "Fast SublimeText-like fuzzy search for JavaScript",
  "keywords": [
    "fuzzy search",
    "fuzzy",
    "search",
    "filter",
    "javascript",
    "node",
    "fast",
    "sublime"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugo-vrijswijk/fuzzysort.git"
  },
  "type": "module",
  "main": "./dist/fuzziersort.cjs",
  "module": "./dist/fuzziersort.js",
  "exports": {
    ".": {
      "import": "./dist/fuzziersort.js",
      "require": "./dist/fuzziersort.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "node test/test.js",
    "test-min": "node test/test.js min",
    "build": "tsdown",
    "postbuild": "cp src/fuzziersort.d.ts dist/fuzziersort.d.ts && cp src/fuzziersort.d.ts dist/fuzziersort.d.cts",
    "dev": "npm run build -- --watch",
    "pretest": "npm run build"
  },
  "files": [
    "dist/*",
    "src/fuzziersort.js"
  ],
  "devDependencies": {
    "tsdown": "^0.15.0"
  }
}
